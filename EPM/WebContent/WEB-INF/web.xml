    <description>Location to store uploaded file</description>
    <param-name>image-file-upload</param-name>
    <param-value>c:\uploads\data\</param-value>
  </context-param>
  <servlet>
    <description>Test-Schedule Template </description>
    <display-name>Scheduler</display-name>
    <servlet-name>Scheduler</servlet-name>
    <servlet-class>Scheduler</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Scheduler</servlet-name>
    <url-pattern>/Scheduler</url-pattern>
  </servlet-mapping>
